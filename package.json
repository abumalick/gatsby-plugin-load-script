{
  "name": "gatsby-plugin-load-script",
  "version": "1.1.0",
  "description": "Add external libraries to your generated html pages",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production yarn build",
    "prepublish": "yarn test",
    "test": "jest",
    "test:cov": "jest && codecov",
    "test:update": "jest --u",
    "test:watch": "jest --watch",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "babel-jest": "^26.0.0",
    "babel-preset-gatsby": "^0.4.0",
    "babel-preset-gatsby-package": "^0.4.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "react": "^16.13.1"
  },
  "peerDependencies": {
    "gatsby": "^2.21.11"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/abumalick/gatsby-plugin-load-script.git"
  },
  "author": "abumalick <abdullah.hilson@gmail.com> (https://asbab.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abumalick/gatsby-plugin-load-script/issues"
  },
  "homepage": "https://github.com/abumalick/gatsby-plugin-load-script",
  "keywords": [
    "sentry",
    "sentry-sdk",
    "gatsby",
    "gatsby-plugin",
    "plugin",
    "script",
    "library"
  ]
}
